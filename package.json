{
	"name": "p-map-lite",
	"version": "1.0.0",
	"description": "A lightweight (180 bytes) p-map version for daily uses.",
	"keywords": [
		"p-map-lite",
		"p-map",
		"p-iteration",
		"promise-map",
		"map",
		"promisify"
	],
	"repository": "rocktimsaikia/p-map-lite",
	"license": "MIT",
	"author": {
		"name": "Rocktim Saikia",
		"email": "rocktimthedev@gmail.com",
		"url": "https://rocktimcodes.site"
	},
	"type": "module",
	"exports": "./src/index.js",
	"types": "src/index.d.ts",
	"files": [
		"src"
	],
	"scripts": {
		"lint": "eslint .",
		"test": "jest .",
		"postinstall": "husky install",
		"prepublishOnly": "pinst --disable",
		"postpublish": "pinst --enable",
		"postversion": "git push && git push --tags"
	},
	"jest": {
		"testTimeout": 30000,
		"transform": {
			"^.+\\.js?$": "babel-jest"
		}
	},
	"devDependencies": {
		"@babel/plugin-transform-runtime": "^7.13.15",
		"@babel/preset-env": "^7.13.15",
		"@types/jest": "^26.0.22",
		"babel-jest": "^26.6.3",
		"eslint": "7.2.0",
		"eslint-config-airbnb-base": "14.2.1",
		"eslint-plugin-import": "2.22.1",
		"husky": "^6.0.0",
		"jest": "^26.6.3",
		"jest-extended": "^0.11.5",
		"pinst": "^2.1.6"
	},
	"engines": {
		"node": ">=12"
	}
}
